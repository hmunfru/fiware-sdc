<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
              http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
        default-autowire="no">

    <bean id="productDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.ProductDaoJpaImpl"/>
    <bean id="productInstanceDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.ProductInstanceDaoJpaImpl"/>
    <bean id="artifactDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.ArtifactDaoJpaImpl"/>  
    <bean id="osDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.OSDaoJpaImpl"/>
    <bean id="productReleaseDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.ProductReleaseDaoJpaImpl"/>
    <bean id="taskDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.TaskDaoJpaImpl"/>
    <bean id="nodeCommandDao" lazy-init="true"
          class="com.telefonica.euro_iaas.sdc.dao.impl.NodeCommandDaoJpaImpl"/>

</beans>
